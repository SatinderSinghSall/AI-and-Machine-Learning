import matplotlib.pyplot as plt

# Data
cities = ["City A", "City B", "City C", "City D", "City E"]

winter_temp = [5, 2, 10, 0, 7]
winter_humidity = [80, 75, 65, 85, 70]

summer_temp = [25, 30, 28, 35, 27]
summer_humidity = [60, 50, 55, 45, 65]

# Figure
plt.figure(figsize=(8, 5))

# Scatter plots (modern look)
plt.scatter(winter_temp, winter_humidity, s=90, alpha=0.85, label="Winter")
plt.scatter(summer_temp, summer_humidity, s=90, alpha=0.85, label="Summer")

# City labels
for i, city in enumerate(cities):
    plt.annotate(
        city,
        (winter_temp[i], winter_humidity[i]),
        textcoords="offset points",
        xytext=(6, 6),
        fontsize=9
    )
    plt.annotate(
        city,
        (summer_temp[i], summer_humidity[i]),
        textcoords="offset points",
        xytext=(6, 6),
        fontsize=9
    )

# Titles and labels
plt.title("Temperature vs Humidity by Season", fontsize=14, pad=10)
plt.xlabel("Temperature (Â°C)", fontsize=11)
plt.ylabel("Humidity (%)", fontsize=11)

# Grid & legend
plt.grid(True, linestyle="--", alpha=0.4)
plt.legend(frameon=False)

plt.tight_layout()
plt.show()







