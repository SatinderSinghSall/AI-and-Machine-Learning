# Vector Normalisation:

# --------------------------------------------
# Vector Normalisation
# --------------------------------------------

vec = np.array([3, 4, 5])
print("Original Vector:", vec, "\n")

# --------------------------------------------
# 1. L2 Norm (Euclidean Norm)
# --------------------------------------------
l2_norm = np.linalg.norm(vec)
print("L2 Norm:", l2_norm)

vec_l2_normalized = vec / l2_norm
print("L2 Normalised Vector:", vec_l2_normalized, "\n")

# --------------------------------------------
# 2. L1 Norm (Manhattan Norm)
# --------------------------------------------
l1_norm = np.sum(np.abs(vec))
print("L1 Norm:", l1_norm)

vec_l1_normalized = vec / l1_norm
print("L1 Normalised Vector:", vec_l1_normalized, "\n")

# --------------------------------------------
# 3. Max Norm (Infinity Norm)
# --------------------------------------------
max_norm = np.max(np.abs(vec))
print("Max Norm:", max_norm)

vec_max_normalized = vec / max_norm
print("Max Normalised Vector:", vec_max_normalized, "\n")

# --------------------------------------------
# 4. Z-score Normalisation
# --------------------------------------------
mean = np.mean(vec)
std = np.std(vec)
print("Mean:", mean)
print("Standard Deviation:", std)

zscore_normalized = (vec - mean) / std
print("Z-score Normalised Vector:", zscore_normalized, "\n")

# --------------------------------------------
# 5. Min-Max Normalisation (0 to 1 scaling)
# --------------------------------------------
min_val = np.min(vec)
max_val = np.max(vec)

minmax_normalized = (vec - min_val) / (max_val - min_val)
print("Min-Max Normalised Vector (0–1):", minmax_normalized)






# Mathematical Functions in NumPy:

import numpy as np

# --------------------------------------------
# Mathematical Functions in NumPy
# --------------------------------------------

arr = np.array([1, 2, 3, 4, 5])
print("Array:", arr, "\n")

# --------------------------------------------
# 1. Basic Arithmetic Functions
# --------------------------------------------

print("Square:", np.square(arr))
print("Cube:", np.power(arr, 3))
print("Square Root:", np.sqrt(arr))
print("Absolute:", np.abs(arr))
print("Reciprocal:", 1 / arr, "\n")

# --------------------------------------------
# 2. Exponential & Logarithmic Functions
# --------------------------------------------

print("Exp (e^x):", np.exp(arr))
print("Natural Log (ln):", np.log(arr))
print("Log base 10:", np.log10(arr))
print("Log base 2:", np.log2(arr), "\n")

# --------------------------------------------
# 3. Trigonometric Functions
# --------------------------------------------

angles = np.array([0, np.pi/2, np.pi])
print("Angles:", angles)

print("sin:", np.sin(angles))
print("cos:", np.cos(angles))
print("tan:", np.tan(angles), "\n")

# Inverse Trigonometric
print("arcsin:", np.arcsin(np.array([0, 1])))
print("arccos:", np.arccos(np.array([1, 0])))
print("arctan:", np.arctan(np.array([0, 1])), "\n")

# --------------------------------------------
# 4. Rounding Functions
# --------------------------------------------

float_arr = np.array([1.2, 2.6, 3.9, 4.4])
print("Float Array:", float_arr)

print("Round:", np.round(float_arr))
print("Floor  :", np.floor(float_arr))
print("Ceil   :", np.ceil(float_arr))
print("Trunc  :", np.trunc(float_arr))
print("Fix    :", np.fix(float_arr), "\n")

# --------------------------------------------
# 5. Statistical Functions
# --------------------------------------------

print("Sum:", np.sum(arr))
print("Mean:", np.mean(arr))
print("Median:", np.median(arr))
print("Std Dev:", np.std(arr))
print("Variance:", np.var(arr))
print("Min:", np.min(arr))
print("Max:", np.max(arr), "\n")

# --------------------------------------------
# 6. Aggregations with Axes (2D)
# --------------------------------------------

matrix = np.array([
    [1, 2, 3],
    [4, 5, 6]
])

print("Matrix:\n", matrix)

print("Column-wise Sum (axis=0):", np.sum(matrix, axis=0))
print("Row-wise Sum (axis=1):", np.sum(matrix, axis=1))
print("Column-wise Mean:", np.mean(matrix, axis=0))
print("Row-wise Mean:", np.mean(matrix, axis=1), "\n")

# --------------------------------------------
# 7. Clipping Values
# --------------------------------------------

print("Clipped Array (min=2, max=4):", np.clip(arr, 2, 4), "\n")

# --------------------------------------------
# 8. Vector & Linear Algebra Functions
# --------------------------------------------

vecA = np.array([1, 2, 3])
vecB = np.array([4, 5, 6])

print("Dot Product:", np.dot(vecA, vecB))
print("Cross Product:", np.cross(vecA, vecB))
print("Norm of A:", np.linalg.norm(vecA), "\n")

# Matrix operations
