# --------------------------------------------

print("Mean of Columns (axis=0):", np.mean(array2D, axis=0))
print("Mean of Rows (axis=1):", np.mean(array2D, axis=1), "\n")

print("Max of Columns (axis=0):", np.max(array2D, axis=0))
print("Max of Rows (axis=1):", np.max(array2D, axis=1), "\n")

print("Min of Columns (axis=0):", np.min(array2D, axis=0))
print("Min of Rows (axis=1):", np.min(array2D, axis=1))






