





import numpy as np
import time


# Execution Performance Comparison:
size = 100_000

# Python List
py_list = list(range(size))
start = time.time()
sq_list = [x == 2 for x in py_list]
end = time.time()
print(f"Python list time = {end - start} seconds.")

# NumPy Array:
np_array = np.array(py_list)
start = time.time()
sq_array = np_array ** 2
end = time.time()
print(f"NumPy array time = {end - start} seconds.")






