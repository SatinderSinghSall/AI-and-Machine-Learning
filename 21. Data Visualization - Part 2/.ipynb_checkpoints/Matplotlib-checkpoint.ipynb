    "np.random.seed(0)\n",
    "scores = np.random.normal(70, 10, 100)\n",
    "plt.hist(\n",
    "    scores,\n",
    "    color=\"skyblue\",\n",
    "    edgecolor=\"black\",\n",
    "    bins=5\n",
    ")\n",
    "plt.ylabel(\"# of Students\")\n",
    "plt.xlabel(\"Scores of Students\")\n"
