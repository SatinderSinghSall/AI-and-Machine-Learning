


import seaborn as sns
from sklearn.datasets import make_blobs
from sklearn.cluster import KMeans





X, y = make_blobs (
    n_samples = 1000,
    n_features = 2,
    centers = 4,
    random_state = 42
)


X
y
X





sns.scatterplot(
    x = X[:, 0],
    y = X[:, 1],
    # c = y
)





K = 4

kmeans = KMeans(
    n_clusters = K,
    random_state = 42
)


kmeans


labels = kmeans.fit_predict(X)
labels


sns.scatterplot(
    x = X[:, 0],
    y = X[: , 1],
    c = labels
)



