


import os

os.environ["OMP_NUM_THREADS"] = "1"
os.environ["LOKY_MAX_CPU_COUNT"] = "1"


import seaborn as sns
from sklearn.datasets import load_iris
from sklearn.cluster import KMeans
import pandas as pd
from sklearn.preprocessing import StandardScaler
from sklearn.decomposition import PCA





iris = load_iris()


X = pd.DataFrame(iris.data)
y = iris.target


X
y
X.head()


X.columns = iris.feature_names


X.head()


y


X.shape


X = iris.data
y = iris.target


X
y





sns.scatterplot(x = X[:, 0], y = X[:, 2])





scaller = StandardScaler()
X_scaled = scaller.fit_transform(X)


X_scaled





pca = PCA(n_components = 2)

pca_data = pca.fit_transform(X_scaled)





# Elbow Method:

wcss = []

for k in range(1, 11):
    kmeans = KMeans(n_clusters = k)
    # kmeans.fit(X_scaled)
    kmeans.fit(pca_data)
    wcss.append(kmeans.inertia_)


wcss


sns.lineplot(x = range(1, 11), y = wcss, marker = 'o')





kmeans = KMeans(n_clusters = 3)
# lables = kmeans.fit_predict(X_scaled)
lables = kmeans.fit_predict(pca_data)

lables


sns.scatterplot(x = X[:, 0], y = X[:, 1], c = lables)


sns.scatterplot(x = X[:, 0], y = X[:, 2], c = y)


pca_data


sns.scatterplot(x = X[:, 0], y = X[:, 1], c = lables)


sns.scatterplot(x = X[:, 0], y = X[:, 1], c = y)


sns.scatterplot(x = kmeans.cluster_centers_[:, 0], y = kmeans.cluster_centers_[:, 1], marker = "x")



