def clean_data(data):
    text_to_num = {"one": 1, "two": 2, "three": 3, "four": 4, "five": 5}
    unique_users = set()
    cleand_data = []

    for user in data:
        # Clean the Rarings: Data Consistency
        raw_rating = str(user["rating"]).strip().lower()
        if raw_rating in text_to_num:
            raw_rating = text_to_num[raw_rating]
        user["rating"] = raw_rating

        # Handle Missing Values:
        raw_age = user.get("age")
        if raw_age == None:
            user["age"] = None

        # Dedublication Data:
        if user["name"].strip() in unique_users:
            continue
        cleand_data.append(user)
    return cleand_data


data = clean_data(data)
print(data)





def get_insights(data):
    # Average Rating:
    total_rating = 0
    for user in data:
        total_rating = total_rating + float(user["rating"])
    average_rating = total_rating / len(data)
    print(f"Average Rating: {average_rating}")

    # Percentage of users with poor rating:
    poor_rating = 0
    for user in data:
        if float(user["rating"]) < 3:
            poor_rating = poor_rating + 1
    result = poor_rating / len(data) * 100
    print(f"% of user with poor rating: {result}%")



get_insights(data)





